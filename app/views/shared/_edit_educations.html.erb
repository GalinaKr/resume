<!--Edit Education Modal-->
<div class="modal fade" id="editEducationModal-<%= profile_education.id %>" tabindex="-1" role="dialog" aria-labelledby="modalLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h1 class="modal-title" id="postJobStep1Label">Edit Education</h1>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <%= form_for :profile_education, url: change_education_path do |f| %>
        <%= render partial: 'shared/form_educations', locals: {profile_education: profile_education, f: f,
                                                                          modal_id: profile_education.id} %>
      <% end %>
    </div>
  </div>
</div>
