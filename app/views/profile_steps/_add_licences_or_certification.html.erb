<!--Add License or Certification Modal-->
<div class="modal fade" id="addLicenseCertificationModal" tabindex="-1" role="dialog" aria-labelledby="modalLabel" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h1 class="modal-title" id="modalLabel"><%= t('interface.steps.certification.form.add_title') %></h1>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">Ã—</span>
        </button>
      </div>
      <%= form_for :profile_certification, url: wizard_path, method: :put do |f| %>
        <%= render partial: 'forms/form_certifications', locals: {profile_certification: nil, f: f} %>
      <% end %>
    </div>
  </div>
</div>
<%= render partial: 'shared/certification_validations', locals: { profile: nil, profile_certification: nil } %>

<script>
  $('#selectCertification').on('click', () => {
    $('#customDescriptionCertificate').val('');
  })
</script>
